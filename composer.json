{
  "name": "education/education",
  "type": "project",
  "license": "MIT",
  "description": "A skeleton to start a new Symfony website",
  "require": {
    "php": "^8.0",
    "ext-ctype": "*",
    "ext-iconv": "*",
    "ext-imagick": "*",
    "asgrim/ofxparser": "*",
    "beberlei/doctrineextensions": "*",
    "composer/package-versions-deprecated": "1.11.99.1",
    "doctrine/annotations": "^1.0",
    "doctrine/doctrine-bundle": "^2.3",
    "doctrine/doctrine-migrations-bundle": "^3.1",
    "doctrine/orm": "^2.8",
    "easycorp/easyadmin-bundle": "3.*",
    "endroid/qr-code-bundle": "*",
    "fardus/traits-symfony": "^1.3",
    "friendsofsymfony/jsrouting-bundle": "*",
    "ghunti/highcharts-php": "^3.0",
    "google/apiclient": "*",
    "phpdocumentor/reflection-docblock": "^5.2",
    "sensio/framework-extra-bundle": "^6.1",
    "sentry/sentry-symfony": "4.*",
    "stof/doctrine-extensions-bundle": "~1.6",
    "symfony/asset": "*",
    "symfony/console": "*",
    "symfony/dotenv": "*",
    "symfony/expression-language": "*",
    "symfony/flex": "*",
    "symfony/form": "*",
    "symfony/framework-bundle": "*",
    "symfony/http-client": "*",
    "symfony/intl": "*",
    "symfony/mailer": "*",
    "symfony/monolog-bundle": "*",
    "symfony/notifier": "*",
    "symfony/process": "*",
    "symfony/property-access": "5.3.*",
    "symfony/property-info": "5.3.*",
    "symfony/proxy-manager-bridge": "5.3.*",
    "symfony/security-bundle": "5.3.*",
    "symfony/serializer": "5.3.*",
    "symfony/string": "*",
    "symfony/swiftmailer-bundle": "*",
    "symfony/translation": "*",
    "symfony/twig-bundle": "5.3.*",
    "symfony/validator": "*",
    "symfony/web-link": "*",
    "symfony/yaml": "*",
    "twig/extra-bundle": "^2.12|^3.0",
    "twig/twig": "^2.12|^3.0",
    "vich/uploader-bundle": "*"
  },
  "require-dev": {
    "composer/semver": "^3.2",
    "composer/xdebug-handler": "^2.0",
    "dama/doctrine-test-bundle": "^6.5",
    "doctrine/doctrine-fixtures-bundle": "^3.4",
    "fakerphp/faker": "^1.14",
    "friendsofphp/php-cs-fixer": "^3.3",
    "liip/test-fixtures-bundle": "^1.6",
    "pheromone/phpcs-security-audit": "^2.0",
    "phpstan/phpstan": "^0.12.9",
    "phpstan/phpstan-deprecation-rules": "^0.12.2",
    "phpstan/phpstan-doctrine": "^0.12.10",
    "phpstan/phpstan-phpunit": "^0.12.8",
    "phpstan/phpstan-symfony": "^0.12.6",
    "phpunit/phpunit": "^9.5",
    "rector/rector": "^0.11.2",
    "roave/security-advisories": "dev-latest",
    "slam/phpstan-extensions": "^5.0",
    "spatie/phpunit-watcher": "^1.23",
    "squizlabs/php_codesniffer": "3.*",
    "symfony/browser-kit": "^5.3",
    "symfony/css-selector": "^5.3",
    "symfony/debug-bundle": "^5.3",
    "symfony/maker-bundle": "^1.15",
    "symfony/phpunit-bridge": "^5.3",
    "symfony/stopwatch": "^5.3",
    "symfony/web-profiler-bundle": "^5.3",
    "symplify/easy-coding-standard": "^9.3",
    "thecodingmachine/phpstan-strict-rules": "^0.12.0",
    "theofidry/alice-data-fixtures": "^1.1"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true,
    "allow-plugins": {
      "endroid/installer": true,
      "pestphp/pest-plugin": true,
      "symfony/flex": true
    }
  },
  "replace": {
    "symfony/polyfill-ctype": "*",
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php72": "*"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    },
    "init-hooks": [
      "sh bin/hooks/install-hooks.sh"
    ],
    "post-install-cmd": [
      "@auto-scripts",
      "@init-hooks"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ]
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "symfony": {
      "allow-contrib": false,
      "require": "5.3.*"
    }
  }
}
