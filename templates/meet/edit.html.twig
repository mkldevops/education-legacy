{% extends 'meet/common.html.twig' %}

{% trans_default_domain 'meet' %}

{% set resources = resources|merge({ ckeditor : true }) %}

{% set title = 'title.edit'|trans({'%name%' : meet}) %}

{% set breadcrumbs = { ('' ~ title) : path('app_meet_edit', {id : meet.id })}|merge(breadcrumbs|default({})) %}

{% block errors %}{{ form_errors(edit_form) }}{% endblock errors %}

{% block body %}
    
    {% embed '_inspina/embed/ibox.html.twig' %}
        {% trans_default_domain "meet" %}
        {% block title 'title.form.edit'|trans %}
        {% block config '' %}
        {% block content %}
            {% include "form.html.twig" %}
        {% endblock %}
    {% endembed %}
{% endblock %}
