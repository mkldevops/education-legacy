{% set resources = resources|merge({ ckeditor : true}) %}

{% extends 'meet/common.html.twig' %}

{% trans_default_domain 'meet' %}

{% set title = 'title.new'|trans %}

{% block errors %}{{ form_errors(form) }}{% endblock errors %}

{% set breadcrumbs = { ('' ~ title) : path('app_meet_new')}|merge(breadcrumbs|default({})) %}

{% block body %}
    
    {% embed '_inspina/embed/ibox.html.twig' %}
        {% trans_default_domain "meet" %}
        {% block title 'title.form.new'|trans %}
        {% block config '' %}
        {% block content %}
            {% include "form.html.twig" %}
        {% endblock %}
    {% endembed %}

{% endblock %}
