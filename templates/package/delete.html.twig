{% extends 'package/common.html.twig' %}

{% trans_default_domain 'package' %}

{% set title = 'package.title.delete'|trans({'%name%' : package}) %}

{% set breadcrumbs = { ('' ~ title) : path('package_show', {id : package.id })}|merge(breadcrumbs|default({})) %}

{% block errors %}{{ form_errors(delete_form) }}{% endblock errors %}


{% block body %}
    {% embed '_inspina/embed/delete.html.twig' %}
        {% trans_default_domain 'package' %}
        {% set nameEntity = 'name_entity'|trans %}
        {% block title 'form.title.delete'|trans %}
    {% endembed %}
{% endblock %}