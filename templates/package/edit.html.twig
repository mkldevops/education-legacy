{% extends 'package/common.html.twig' %}

{% trans_default_domain 'package' %}

{% set title = 'package.title.edit'|trans({'%name%' : package}) %}

{% set breadcrumbs = { ('' ~ title) : path('app_package_edit', {id : package.id })}|merge(breadcrumbs|default({})) %}

{% block errors %}{{ form_errors(edit_form) }}{% endblock errors %}

{% block body %}

    {% embed '_inspina/embed/ibox.html.twig' %}
        {% trans_default_domain "package" %}
        {% block title 'form.title.edit'|trans %}
        {% block config '' %}
        {% block content %}
            {% include "form.html.twig" %}
        {% endblock %}
    {% endembed %}
{% endblock %}
