{% extends 'class_school/common.html.twig' %}

{% trans_default_domain 'class_school' %}

{% set title = 'title.edit'|trans({'%name%' : classschool }) %}

{% set breadcrumbs = { ('' ~ title) : path('app_grade_edit', {id : classschool.id })}|merge(breadcrumbs|default({})) %}

{% block errors %}{{ form_errors(edit_form) }}{% endblock errors %}

{% block body %}

    {% embed '_inspina/embed/ibox.html.twig' %}
        {% trans_default_domain "class_school" %}
        {% block title 'title.form.edit'|trans %}
        {% block config '' %}
        {% block content %}
            {% include "form.html.twig" %}
        {% endblock %}
    {% endembed %}
{% endblock %}
