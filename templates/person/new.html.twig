{% extends 'person/common.html.twig' %}

{% trans_default_domain 'person' %}

{% set title = 'title.new'|trans %}

{% set breadcrumbs = { ('' ~ title) : path('app_person_new')}|merge(breadcrumbs|default({})) %}

{% block errors %}{{ form_errors(form) }}{% endblock errors %}

{% block body %}

    {% embed '_inspina/embed/ibox.html.twig' %}
        {% trans_default_domain "person" %}
        {% block title 'title.form.new'|trans %}
        {% block config '' %}
        {% block content %}
            {{ form(form) }}
        {% endblock %}
    {% endembed %}

{% endblock %}
