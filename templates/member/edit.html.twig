{% extends 'member/common.html.twig' %}

{% trans_default_domain 'member' %}

{% set title = 'title.edit'|trans({'%name%' : member}) %}

{% set breadcrumbs = { ('' ~ title) : path('app_member_edit', {id : member.id })}|merge(breadcrumbs|default({})) %}

{% block errors %}{{ form_errors(edit_form) }}{% endblock errors %}

{% block body %}

    {% embed '_inspina/embed/ibox.html.twig' %}
        {% trans_default_domain "member" %}
        {% block title 'title.form.edit'|trans %}
        {% block config '' %}
        {% block content %}
            {% include "member/_form.html.twig" %}
        {% endblock %}
    {% endembed %}
{% endblock %}
