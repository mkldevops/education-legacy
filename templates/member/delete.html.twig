{% extends 'member/common.html.twig' %}

{% trans_default_domain 'member' %}

{% set title = 'title.delete'|trans({'%name%' : member }) %}

{% set breadcrumbs = { ('' ~ title) : path('app_member_delete', {id : member.id })}|merge(breadcrumbs|default({})) %}

{% block errors %}{{ form_errors(delete_form) }}{% endblock errors %}

{% block body %}
    {% embed '_inspina/embed/delete.html.twig' %}
        {% trans_default_domain 'member' %}
        {% set nameEntity = 'member'|trans %}
        {% block title 'title.form.delete'|trans %}
    {% endembed %}
{% endblock %}
