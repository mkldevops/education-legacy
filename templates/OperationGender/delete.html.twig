{% extends 'OperationGender:common.html.twig' %}

{% trans_default_domain 'operationgender' %}

{% set title = 'title.delete'|trans({'%name%' : operationgender }) %}

{% set breadcrumbs = { ('' ~ title) : path('app_operation-gender_delete', {id : operationgender.id })}|merge(breadcrumbs|default({})) %}

{% block errors %}{{ form_errors(delete_form) }}{% endblock errors %}

{% block body %}
    {% embed '_inspina/embed/delete.html.twig' %}
        {% trans_default_domain 'operationgender' %}
        {% set nameEntity = 'operationgender'|trans %}
        {% block title 'title.form.delete'|trans %}
    {% endembed %}
{% endblock %}
