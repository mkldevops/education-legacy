{% extends 'OperationGender:common.html.twig' %}

{% trans_default_domain 'operationgender' %}

{% set title = 'title.show'|trans({'%name%' : operationgender}) %}

{% set breadcrumbs = { ('' ~ title) : path('app_operation-gender_show', {id : operationgender.id })}|merge(breadcrumbs|default({})) %}

{% block body %}
    {% embed '_inspina/embed/ibox.html.twig' %}
        {% trans_default_domain "operationgender" %}
        {% block title 'title.header.info'|trans({'%name%' : operationgender }) %}
        {% block config '' %}
        {% block content %}
            <dl class="dl-horizontal">
                <dt>{%trans%}Id{%endtrans%}</dt>
                <dd>
                    {{ operationgender.id }}
                </dd>
                
                <dt>{%trans%}Name{%endtrans%}</dt>
                <dd>
                    {{ operationgender.name }}
                </dd>
                
                <dt>{%trans%}Code{%endtrans%}</dt>
                <dd>
                    {{ operationgender.code }}
                </dd>
                
                <dt>{%trans%}Created{%endtrans%}</dt>
                <dd>
                    {{ operationgender.created|date('d/m/Y H:i:s') }}
                </dd>
                
                <dt>{%trans%}Status{%endtrans%}</dt>
                <dd>
                    {{ operationgender.status }}
                </dd>
            </dl>
        {% endblock %}
    {% endembed %}

{% endblock %}
