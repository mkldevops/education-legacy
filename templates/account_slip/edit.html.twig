{% trans_default_domain 'account_slip' %}

{% set resources = resources|merge({ ckeditor : true }) %}

{% extends 'account_slip/common.html.twig' %}

{% set title = 'title.edit'|trans({'%name%' : accountslip }) %}

{% set breadcrumbs = { ('' ~ title) : path('app_account_slip_edit', { id : accountslip.id })}|merge(breadcrumbs|default({})) %}

{% block errors %}{{ form_errors(edit_form) }}{% endblock errors %}

{% block body %}

    {% embed '_inspina/embed/ibox.html.twig' %}
        {% trans_default_domain "account_slip" %}
        {% block title 'title.form_edit'|trans %}
        {% block config '' %}
        {% block content %}
            {% include "form.html.twig" %}
        {% endblock %}
    {% endembed %}

{% endblock %}
